{
  "name": "gts-symbiota-mobile-checklist",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Green Theory Studio's Symbiota Mobile Checklist",
  "author": "Ben Brandt <ben@greentheorystudio.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "icons": "icongenie generate -i app-icon.png --skip-trim",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "npm run copy:sql:wasm && quasar dev",
    "dev:ios": "npm run copy:sql:wasm && quasar dev -m capacitor -T ios",
    "dev:android": "npm run copy:sql:wasm && quasar dev -m capacitor -T android",
    "build": "quasar build",
    "copy:sql:wasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm public/assets",
    "remove:sql:wasm": "rimraf public/assets/sql-wasm.wasm",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "^7.0.3",
    "@capacitor/core": "^8.0.0",
    "@capacitor/file-transfer": "^2.0.2",
    "@capacitor/filesystem": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/toast": "^8.0.0",
    "@ionic/pwa-elements": "^3.3.0",
    "@quasar/extras": "^1.17.0",
    "jeep-sqlite": "^2.8.0",
    "jszip": "^3.10.1",
    "pinia": "^3.0.4",
    "quasar": "^2.18.6",
    "sql.js": "1.12.0",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.0",
    "@quasar/app-vite": "^2.4.0",
    "@types/node": "^25.0.3",
    "autoprefixer": "^10.4.23",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.1.2",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
